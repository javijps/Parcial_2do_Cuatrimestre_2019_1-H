#ifndef AUXILIARCLIENTE_H_
#define AUXILIARCLIENTE_H_

#include "utn.h"

typedef struct{

	int idCliente;
	int status;
	int contadorPedidos;
	float acumuladorPedidos;

} sAuxiliarCliente;


void imprimirUnClienteAux(sAuxiliarCliente aAuxiliarCliente);
int initClienteAuxiliar(sAuxiliarCliente *aAuxiliarCliente,int len);
int buscarClienteAuxiliarPorId(sAuxiliarCliente *aAuxiliarCliente,int len, int idCliente);
void imprimirContadorAuxCliente(sAuxiliarCliente aAuxiliarCliente);



#endif /* AUXILIARCLIENTE_H_ */
